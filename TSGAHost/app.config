<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="GABehavior">
                    <serviceMetadata httpGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="GABehavior" name="TSGALib.TSGAService">
                <endpoint address="GA" binding="wsDualHttpBinding" bindingConfiguration=""
                    name="GAEP" contract="TSGALib.ITSGA" listenUriMode="Explicit" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:7065/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>